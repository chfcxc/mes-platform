<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.UserDepartmentAssign" table="system_user_department_assign">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="departmentId" column="enterprise_department_id" type="long" />
		<property name="userId" column="system_user_id" type="long" />
		<property name="identity" column="identity" type="int" />
	</class>
</hibernate-mapping>


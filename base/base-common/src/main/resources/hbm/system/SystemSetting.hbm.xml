<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.SystemSetting" table="system_settings">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="depict" column="depict" type="string" />
		<property name="settingKey" column="setting_key" type="string" />
		<property name="settingValue" column="setting_value" type="string" />
		<property name="unit" column="unit" type="string" />
		<property name="remark" column="remark" type="string" />
	</class>
</hibernate-mapping>



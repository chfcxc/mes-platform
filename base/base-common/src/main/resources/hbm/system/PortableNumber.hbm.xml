<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.PortableNumber" table="system_portable_number">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="mobile" column="mobile" type="string" />
		<property name="operatorCode" column="operator_code" type="string" />
		<property name="isDelete" column="is_delete" type="boolean" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
	</class>
</hibernate-mapping>

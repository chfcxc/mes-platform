<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.ContactGroup" table="system_contact_group">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="groupName" column="group_name" type="string" />
		<property name="groupType" column="group_type" type="int" />
		<property name="userId" column="user_id" type="long" />
		<property name="enterpriseId" column="enterprise_id" type="long" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="isDelete" column="is_delete" type="boolean" />
	</class>
</hibernate-mapping>
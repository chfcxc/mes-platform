<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.SectionNumber" table="system_section_number">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="number" column="number" type="string" />
		<property name="provinceCode" column="province_code" type="string" />
		<property name="provinceName" column="province_name" type="string" />
		<property name="city" column="city" type="string" />
		<property name="operatorCode" column="operator_code" type="string" />
		<property name="isDelete" column="is_delete" type="boolean" />
	</class>
</hibernate-mapping>

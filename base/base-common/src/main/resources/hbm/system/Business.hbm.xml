<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Business" table="system_business">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="businessName" column="business_name" type="string" />
		<property name="businessNameSimple" column="business_name_simple" type="string" />
		<property name="businessCode" column="business_code" type="string" />
		<property name="businessIndex" column="business_index" type="int" />
		<property name="createTime" column="create_time" type="timestamp" />
	</class>
</hibernate-mapping>

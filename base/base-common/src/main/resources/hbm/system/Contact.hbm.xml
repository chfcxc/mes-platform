<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Contact" table="system_contact">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="mobile" column="mobile" type="string" />
		<property name="userId" column="user_id" type="long" />
		<property name="realName" column="real_name" type="string" />
		<property name="email" column="email" type="string" />
		<property name="qq" column="qq" type="string" />
		<property name="birthday" column="birthday" type="timestamp" />
		<property name="company" column="company" type="string" />
		<property name="position" column="position" type="string" />
		<property name="companyAddress" column="company_address" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="isDelete" column="is_delete" type="boolean" />
	</class>
</hibernate-mapping>
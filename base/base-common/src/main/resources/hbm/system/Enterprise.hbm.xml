<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Enterprise" table="system_enterprise">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="nameCn" column="name_cn" type="string" />
		<property name="type" column="type" type="string" />
		<property name="clientNumber" column="client_number" type="string" />
		<property name="linkman" column="linkman" type="string" />
		<property name="mobile" column="mobile" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="operatorId" column="operator_id" type="long" />
		<property name="isDelete" column="is_delete" type="boolean" />
		<property name="email" column="email" type="string" />
		<property name="isVip" column="is_vip" type="boolean" />
		<property name="telephone" column="telephone" type="string" />
		<property name="address" column="address" type="string" />
		<property name="remark" column="remark" type="string" />
		<property name="authority" column="authority" type="int" />
		<property name="valueAddedService" column="value_added_service" type="string" />
		<property name="viplevel" column="viplevel" type="int" />
		<property name="serviceType" column="service_type" type="string" />
		<property name="startClientSelectTime" column="start_client_select_time" type="timestamp" />
		<property name="endClientSelectTime" column="end_client_select_time" type="timestamp" />
	</class>
</hibernate-mapping>
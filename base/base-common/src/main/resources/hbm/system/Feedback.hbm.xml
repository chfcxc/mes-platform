<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Feedback" table="system_feedback">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="feedbackContent" column="feedback_content" type="string" />
		<property name="email" column="email" type="string" />
		<property name="mobile" column="mobile" type="string" />
		<property name="qq" column="qq" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="operatorId" column="operator_id" type="long" />
		<property name="handleState" column="handle_state" type="int" />
		<property name="handleUserId" column="handle_user_id" type="long" />
		<property name="handleTime" column="handle_time" type="timestamp" />
	</class>
</hibernate-mapping>



<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Role" table="system_role">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="roleName" column="role_name" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="isDelete" column="is_delete" type="boolean" />
		<property name="remark" column="remark" type="string" />
		<property name="roleType" column="role_type" type="string" />
	</class>
</hibernate-mapping>

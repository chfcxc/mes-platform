<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.system.Resource" table="system_resource">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="resourceType" column="resource_type" type="string" />
		<property name="resourceCode" column="resource_code" type="string" />
		<property name="resourceName" column="resource_name" type="string" />
		<property name="resourcePath" column="resource_path" type="string" />
		<property name="resourceIndex" column="resource_index" type="int" />
		<property name="resourceIcon" column="resource_icon" type="string" />
		<property name="parentResourceId" column="parent_system_resource_id" type="long" />
		<property name="resourceFor" column="resource_for" type="string" />
		<property name="resourceForCustomer" column="resource_for_customer" type="string" />
		<property name="businessType" column="business_type" type="string" />
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.fms.FmsTemplate" table="fms_template">
		<id name="id" column="id" type="string"></id>
		<property name="content" column="content" type="string" />
		<property name="variable" column="variable" type="string" />
		<property name="submitTime" column="submit_time" type="timestamp" />
		<property name="userId" column="user_id" type="long" />
		<property name="templateType" column="template_type" type="int" />
		<property name="createTime" column="create_time" type="timestamp" />
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.emay.eucp.common.moudle.db.fms.FmsAccount" table="fms_account">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="enterpriseId" column="enterprise_id" type="long" />
		<property name="serviceCodeId" column="service_code_id" type="long" />
		<property name="appId" column="app_id" type="string" />
		<property name="balance" column="balance" type="long" />
		<property name="remark" column="remark" type="string" />
	</class>
</hibernate-mapping>
